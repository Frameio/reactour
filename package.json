    "html-webpack-plugin": "3.2.0",
    "prettier": "1.13.7",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "reshake": "1.0.8",
    "rimraf": "2.6.2",
    "style-loader": "0.21.0",
