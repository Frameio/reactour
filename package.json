{
  "name": "@frameio/reactour",
  "version": "1.8.13",
  "description": "Tourist Guide into your React Components",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run lib",
    "start": "webpack-dev-server --mode development --open",
